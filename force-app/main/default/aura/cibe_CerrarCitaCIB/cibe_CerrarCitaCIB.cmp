<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" controller="CIBE_PathEventCIBController" access="global">
    <aura:html tag="style">
        .slds-backdrop {
        background: rgba(43, 40, 38, 0) !important;  
        }
        .slds-fade-in-open {
        display:none !important;
       }
	</aura:html>
    <lightning:notificationsLibrary aura:id="notifLib"/>
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="response" type="String"/>
</aura:component>