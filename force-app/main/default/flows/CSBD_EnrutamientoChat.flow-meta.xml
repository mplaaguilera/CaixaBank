                    <elementReference>getLiveChatTranscript.CSBD_Now_Codigo_Producto__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>buscarSkillProducto</targetReference>
            </connector>
            <label>Sí</label>
        </rules>
    </decisions>
