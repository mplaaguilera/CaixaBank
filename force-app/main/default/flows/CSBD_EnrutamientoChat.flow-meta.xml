    <decisions>
        <name>decisionRequerirSkillIdioma</name>
        <label>¿Requiere skill de idioma?</label>
        <locationX>769</locationX>
        <locationY>453</locationY>
        <defaultConnector>
            <targetReference>setProductosQueRequierenSkill</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>outcomeRequiereSkillIdioma</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>getLiveChatTranscript.CC_Idioma__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>ca</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>getLiveChatTranscript.CC_Idioma__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>es</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>getLiveChatTranscript.CC_Idioma__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>en</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>buscarSkillIdiomaRequerido</targetReference>
            </connector>
            <label>Sí</label>
        </rules>
    </decisions>
    <decisions>
        <name>decisionRequerirSkillProducto</name>
