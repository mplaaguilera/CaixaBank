@isTest
public with sharing class AV_TabManagementEvent_Controller_Test {

    @TestSetup
	static void setup() {
        Account centro = AV_TestHelper.createCaixaCenter();
        User u = AV_TestHelper.createUser('Standard User');
        Contact empleado = AV_TestHelper.createEmployee(centro,u);
        Account cliente = AV_TestHelper.createCustomer();
        Task aviso = AV_TestHelper.createTareaExternalIdWithCustomer(u, null, cliente);
        aviso.RecordTypeId = AV_AppUtilities.getRecordType(AV_AppConstants.OBJECT_NAME_TASK, AV_AppConstants.TASKAVISOS_RT).Id;
        Database.update(aviso);
        Datetime currentDatetime = Datetime.now();
        Event e = AV_TestHelper.createEventConCliente(u, currentDatetime, cliente);
        Test.setMock(HttpCalloutMock.class, new AV_MockCallout_Test('GCF_CAMBIOEMPLEADO','OK'));
    }

    @isTest
    public static void validateUpdateEvent() {
        Account cliente = [Select Id From Account Where FirstName = 'FirstName' and LastName = 'LastName' and AV_NumPerso__c = '123' limit 1];
        Event e = [Select Id, CSBD_Evento_Estado__c from Event where Subject = 'Call' and AV_Tipo__c = 'CTO' and WhatId = :cliente.Id limit 1];
        Test.startTest();
        Test.setMock(HttpCalloutMock.class, new AV_MockCallout_Test('GCF_CAMBIOEMPLEADO','OK'));
        String result = AV_TabManagementEvent_Controller.updateEvent(e.Id, 'Test comment');
        Test.stopTest();
        Event eventAU = [Select Id, CSBD_Evento_Estado__c from Event where Subject = 'Call' and AV_Tipo__c = 'CTO' and WhatId = :cliente.Id limit 1];
        System.assertEquals('OK', result, 'Event not properly updated.');
    }

    @isTest
    public static void validateGetEventData() {
        Account cliente = [Select Id From Account Where FirstName = 'FirstName' and LastName = 'LastName' and AV_NumPerso__c = '123' limit 1];
        Event e = [Select Id, CSBD_Evento_Estado__c from Event where Subject = 'Call' and AV_Tipo__c = 'CTO' and WhatId = :cliente.Id limit 1];
        Test.startTest();
        AV_TabManagementEvent_Controller.EventWrapper result = AV_TabManagementEvent_Controller.getEventData(e.Id);
        Test.stopTest();
        System.assertNotEquals(null, result, 'Could not get event data.');
    }

    @isTest
    public static void validateDoPendingTasksExist() {
        Account cliente = [Select Id From Account Where FirstName = 'FirstName' and LastName = 'LastName' and AV_NumPerso__c = '123' limit 1];
        Event e = [Select Id, CSBD_Evento_Estado__c from Event where Subject = 'Call' and AV_Tipo__c = 'CTO' and WhatId = :cliente.Id limit 1];
        Test.startTest();
        Boolean result = AV_TabManagementEvent_Controller.doPendingTasksExist(e.Id);
        Test.stopTest();
        System.assertEquals(true, result, 'Could not get pending task.');
    }
    
    @isTest
    public static void validateGetEventDataError() {
        Test.startTest();
        AV_TabManagementEvent_Controller.EventWrapper result = AV_TabManagementEvent_Controller.getEventData('123a');
        Test.stopTest();
        System.assert(result.tipo == null && result.comment == null);
    }

    @isTest
    public static void validateDoPendingTasksExistError() {
        Test.startTest();
        Boolean result = AV_TabManagementEvent_Controller.doPendingTasksExist('123a');
        Test.stopTest();
        System.assert(!result);
        
    }
    
