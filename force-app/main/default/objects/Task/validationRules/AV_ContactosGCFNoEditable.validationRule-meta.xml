<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AV_ContactosGCFNoEditable</fullName>
    <active>true</active>
    <description>El usuario CaixaBank no puede editar ningún campo para el RT &apos;Contactos GCF&apos;</description>
    <errorConditionFormula>AND(
$Profile.Name =&quot;AV_Usuario_CaixaBank&quot;,
RecordType.DeveloperName = &apos;AV_ContactosGCF&apos;,
RecordType.DeveloperName != &apos;CSBD_Task&apos;,
NOT($Permission.AV_AvoidBulkApi),
AND(
!$Permission.AV_ByPassValidationRules,
!$Permission.CBK_BypassValidationRules
)
)</errorConditionFormula>
    <errorMessage>Información no editable</errorMessage>
</ValidationRule>
