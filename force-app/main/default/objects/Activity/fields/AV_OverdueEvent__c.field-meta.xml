<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AV_OverdueEvent__c</fullName>
    <formula>AND((MILLISECOND(TIMEVALUE(ActivityDateTime)) + (DurationInMinutes * 60 * 1000)) &gt; MILLISECOND(TIMEVALUE(TIMENOW())), ActivityDateTime &lt; NOW(), TEXT(CSBD_Evento_Estado__c) = &apos;Pendiente&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Overdue Event</label>
    <type>Checkbox</type>
</CustomField>
