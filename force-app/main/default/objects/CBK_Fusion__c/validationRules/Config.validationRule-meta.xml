<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Config</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
(ISBLANK(TEXT(Tipo__c))),
(ISBLANK(TEXT(Config__c)))
)</errorConditionFormula>
    <errorMessage>Debe seleccionarse Tipo de fusión o informarse una configuración</errorMessage>
</ValidationRule>
