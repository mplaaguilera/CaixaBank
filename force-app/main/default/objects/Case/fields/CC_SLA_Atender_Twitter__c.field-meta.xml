<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CC_SLA_Atender_Twitter__c</fullName>
    <description>Calculamos si se cumple el SLA de atender el Twitter en menos de 30 minutos.</description>
    <externalId>false</externalId>
    <formula>IF((CC_Tiempo_Respuesta_Twitter__c &lt;= 30),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>SLA Twitter</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
