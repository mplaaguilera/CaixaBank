<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CIBE_DiasUltimaGestion__c</fullName>
    <description>Días desde la última modificación de la oportunidad</description>
    <externalId>false</externalId>
    <formula>IF((AV_FechaModificacion__c &lt; TODAY()), TODAY() - AV_FechaModificacion__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Días ult. gestión</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
