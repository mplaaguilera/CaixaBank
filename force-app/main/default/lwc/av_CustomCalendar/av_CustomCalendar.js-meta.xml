<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__RecordPage</target>
        <target>lightning__AppPage</target>
        <target>lightning__HomePage</target>
        <target>lightning__Tab</target>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="gestorOverwrite" type="String" label="OverwriteTimeGestor"
                description="Indicate whether the manager can ovewirte a slot time" role="inputOnly" />
            <property name="timeRange" type="Integer" label="Duration"
                description="Event duration in minutes." role="inputOnly" />
            <property name="currentDate" type="Date" label="Date" description="Event date."
                role="inputOnly" />
            <property name="userId" type="String" label="AssignId"
                description="Id of user assigned to event." role="inputOnly" />
            <property name="evSubject" type="String" label="Subject" description="Event subject."
                role="inputOnly" />
            <property name="finalDate" type="Date" label="Final Date" description="Final date."
                role="outputOnly" />
            <property name="initTime" type="Datetime" label="Init Datetime"
                description="Event init datetime." role="outputOnly" />
            <property name="endTime" type="Datetime" label="End Datetime"
                description="Event end datetime." role="outputOnly" />
            <property name="checkTime" type="Boolean" label="Check Time" description="Check Time."
                role="outputOnly" />
            <property name="invalidateTime" type="Boolean" label="Invalidate Time"
                description="Invalidate Time." role="outputOnly" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>